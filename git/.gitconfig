	name = Tyler Kindy
	signingkey = 9DA683C4C390597C50727C81C225D68C6B87D927
[core]
	hooksPath = /Users/tkindy/.git_hooks
	excludesfile = /Users/tkindy/.gitignore_global
  # pager = diff-so-fancy | less --tabs=4 -RFX
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  navigate = true
  light = false
[diff]
  colorMoved = default
[merge]
  conflictstyle = diff3
[remote "origin"]
	prune = true
[hub]
	host = git.hubteam.com
[commit]
	gpgsign = true
[gpg]
	program = gpg
[pull]
	rebase = true
[diff-so-fancy]
	markEmptyLines = false
	stripLeadingSymbols = false
[advice]
	detachedHead = false
[color "diff-highlight"]
	newNormal = green
	newHighlight = green 22
